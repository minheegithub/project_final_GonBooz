<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">



<mapper namespace="com.gb.wn.main.DAO.MusicDAO">
	<select id="findMusic" resultType="String">
	SELECT m_url
	FROM musicList where m_name = #{selectedName}
	</select>
	
</mapper>
